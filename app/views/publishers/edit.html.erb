<h1>Fill out the field below to edit the publisher details: </h1>
<%= render partial: "layouts/error", locals: {object: @publisher} %>

<%= form_for @publisher do |p| %>
    
    <%= p.label "name" %><br>
    <%= p.text_field :name %><br>

    <%= p.label "based_in" %><br>
    <%= p.text_field :based_in %><br>

    <%= p.label "awards" %><br>
    <%= p.text_field :awards %><br>
    
    <%= p.submit %>
<% end %>