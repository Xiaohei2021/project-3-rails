<%= form_for([@game, @review]) do |f|  %>
    
    <%= f.label "Please rate the outstanding aspect of the game (1-5)" %><br>
    <%= f.select :props, options_for_select(1..5) %><br>
   
    <%= f.label "Comment" %><br>
    <%= f.text_area :comment%><br>

    <%= f.label "Please rate the deficinecy aspect of the game (1-5)" %><br>
    <%= f.select :boos, options_for_select(1..5) %><br>

    <%= f.label "Comment" %><br>
    <%= f.text_area :comment%><br>

    <%= f.hidden_field :game_id, value: @game.id%>
    <%= f.submit %>
<%end %>